sub SAVELORD{
	open(IN,"$LOG_FOLDER2/$FORM{'pname'}.cgi")|| &ERROR('NameError','IDが間違っているか、登録されていません。');
	@PLVALS=<IN>;
	close(IN);
	foreach(@PLVALS){($NAME,$PLVAL[0],$PLVAL[1],$PLVAL[2],$PLVAL[3],$PLVAL[4],$PLVAL[5],$PLVAL[6],$PLVAL[7],$PLVAL[8],$PLVAL[9],$PLVAL[10],$PLVAL[11],$PLVAL[12],$PLVAL[13],$PLVAL[14],$PLVAL[15],$PLVAL[16],$PLVAL[17],$PLVAL[18],$PLVAL[19],$PLVAL[20],$PLVAL[21],$PLVAL[22],$PLVAL[23],$PLVAL[24],$PLVAL[25],$PLVAL[26],$PLVAL[27],$PLVAL[28],$PLVAL[29],$PLVAL[30],$PLVAL[31],$PLVAL[32],$PLVAL[33],$PLVAL[34],$PLVAL[35],$PLVAL[36],$PLVAL[37],$PLVAL[38],$PLVAL[39],$PLVAL[40],$PLVAL[41],$PLVAL[42],$PLVAL[43],$PLVAL[44],$PLVAL[45],$PLVAL[46],$PLVAL[47],$PLVAL[48],$PLVAL[49],$PLVAL[50],$PLVAL[51],$PLVAL[52],$PLVAL[53],$PLVAL[54],$PLVAL[55],$PLVAL[56],$PLVAL[57],$PLVAL[58],$PLVAL[59],$PLVAL[60],$PLVAL[61],$PLVAL[62],$PLVAL[63],$PLVAL[64],$PLVAL[65],$PLVAL[66],$PLVAL[67],$PLVAL[68],$PLVAL[69],$PLVAL[70],$PLVAL[71],$PLVAL[72],$PLVAL[73],$PLVAL[74],$PLVAL[75],$PLVAL[76],$PLVAL[77],$PLVAL[78],$PLVAL[79],$PLVAL[80])=split(/<>/);}


	&ERROR('NameError','IDが間違っているか、登録されていません。') if $NAME ne "$FORM{'pname'}";
	&ERROR('PwdError','パスワードが間違っている恐れがあります。') if crypt ($FORM{'pass'},eb) ne "$PLVAL[2]";

	require './vaback.pl';
	&backplayerlist;

	&LOCK;
	dbmopen (%PL,"$DBM_P",0666);
		$PL{"$FORM{'pname'}"} = ("$PLVAL[0] $PLVAL[1] $PLVAL[2] $PLVAL[3] $PLVAL[4] $PLVAL[5] 0 $PLVAL[7] $PLVAL[8] $PLVAL[9] $PLVAL[10] $PLVAL[11] $PLVAL[12] $PLVAL[13] 0 $PLVAL[15] $PLVAL[16] $PLVAL[17] $PLVAL[18] $PLVAL[19] $PLVAL[20] $PLVAL[21] $PLVAL[22] $PLVAL[23] $PLVAL[24] 0 $DATE $PLVAL[27] $PLVAL[28] $PLVAL[29] $PLVAL[30] $PLVAL[31] $PLVAL[32] $PLVAL[33] $PLVAL[34] $PLVAL[35] $PLVAL[36] $PLVAL[37] $PLVAL[38] $PLVAL[39] $PLVAL[40] $PLVAL[41] $PLVAL[42] $PLVAL[43] $PLVAL[44] $PLVAL[45] $PLVAL[46] $PLVAL[47] $PLVAL[48] $PLVAL[49] $PLVAL[50] $PLVAL[51] $PLVAL[52] $PLVAL[53] $PLVAL[54] $PLVAL[55] $PLVAL[56] $PLVAL[57] $PLVAL[58] $PLVAL[59] $PLVAL[60] $PLVAL[61] $PLVAL[62] $PLVAL[63] $PLVAL[64] $PLVAL[65] $PLVAL[66] $PLVAL[67] $PLVAL[68] $PLVAL[69] $PLVAL[70] $PLVAL[71] $PLVAL[72] $PLVAL[73] $PLVAL[74] $PLVAL[75] $PLVAL[76] $PLVAL[77] $PLVAL[78] $PLVAL[79] $PLVAL[80]");
	dbmclose (%PL);
	&UNLOCK;
}

1;
